#!groovy

@Library("Infrastructure")

def type = "java"
def product = "data-extractor"
def component = "job"

withPipeline(type, product, component) {
  env.TESTCONTAINERS_RYUK_DISABLED = 'true'
  enableDockerBuild()
  installCharts()
  disableLegacyDeployment()
  enableAksStagingDeployment()
  nonServiceApp()

}
